package com.deloitte.jio.model;

import java.util.List;



import com.deloitte.jio.entity.Customer;



public class Response {
	
	
	private Customer customer;
	private Response2 resp;
	private List<JioMart> jiom;
	private Recharge recharge;
	public Response() {
		super();
		// TODO Auto-generated constructor stub
	}
	
	
	
	
	public Response(Customer customer, Response2 resp) {
		super();
		this.customer = customer;
		this.resp = resp;
	}




	public Response(Customer customer, Response2 resp, List<com.deloitte.jio.model.JioMart> jiom, Recharge recharge) {
		super();
		this.customer = customer;
		this.resp = resp;
		this.jiom = jiom;
		this.recharge = recharge;
	}
	public Customer getCustomer() {
		return customer;
	}
	public void setCustomer(Customer customer) {
		this.customer = customer;
	}
	public Response2 getResp() {
		return resp;
	}
	public void setResp(Response2 resp) {
		this.resp = resp;
	}
	public List<JioMart> getJiom() {
		return jiom;
	}
	public void setJiom(List<JioMart> jiom) {
		this.jiom = jiom;
	}
	public Recharge getRecharge() {
		return recharge;
	}
	public void setRecharge(Recharge recharge) {
		this.recharge = recharge;
	}
	
	
}
