package com.deloitte.jio.model;

import java.util.List;

import com.deloitte.jio.entity.Customer;


public class Response {
	
	private List<JioMart> jiom;
	private Customer customer;
	private Recharge recharge;
	public Response() {
		super();
		// TODO Auto-generated constructor stub
	}
	public Response(List<JioMart> jiom, Customer customer, Recharge recharge) {
		super();
		this.jiom = jiom;
		this.customer = customer;
		this.recharge = recharge;
	}
	public List<JioMart> getJiom() {
		return jiom;
	}
	public void setJiom(List<JioMart> jiom) {
		this.jiom = jiom;
	}
	public Customer getCustomer() {
		return customer;
	}
	public void setCustomer(Customer customer) {
		this.customer = customer;
	}
	public Recharge getRecharge() {
		return recharge;
	}
	public void setRecharge(Recharge recharge) {
		this.recharge = recharge;
	}
	
}
